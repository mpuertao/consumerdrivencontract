{
  "id" : "d3395718-68cb-4943-b22d-dac8a36e4e59",
  "request" : {
    "url" : "/credit-scores",
    "method" : "POST",
    "headers" : {
      "Content-Type" : {
        "matches" : "application/json.*"
      }
    },
    "bodyPatterns" : [ {
      "matchesJsonPath" : "$[?(@.['citizenNumber'] == 4444)]"
    }, {
      "matchesJsonPath" : "$[?(@.['requestDate'] =~ /(\\d\\d\\d\\d)-(0[1-9]|1[012])-(0[1-9]|[12][0-9]|3[01])/)]"
    }, {
      "matchesJsonPath" : "$[?(@.['uuid'] =~ /[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}/)]"
    } ]
  },
  "response" : {
    "status" : 200,
    "body" : "{\"score\":\"LOW\",\"uuid\":\"{{{jsonpath this '$.uuid'}}}\"}",
    "headers" : {
      "Content-Type" : "application/json"
    },
    "transformers" : [ "response-template" ]
  },
  "uuid" : "d3395718-68cb-4943-b22d-dac8a36e4e59"
}